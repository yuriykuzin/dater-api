{
  "name": "datewizard",
  "version": "1.0.0",
  "description": "Datewizard",
  "dependencies": {
    "@koa/cors": "2.2.2",
    "koa": "2.6.1",
    "koa-bodyparser": "4.2.1",
    "koa-helmet": "4.0.0",
    "koa-passport": "4.1.1",
    "koa-router": "7.4.0",
    "koa-session": "5.10.0",
    "passport-facebook": "2.1.1",
    "passport-local": "1.0.0",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@types/koa": "2.0.46",
    "@types/koa-bodyparser": "4.2.0",
    "@types/koa-helmet": "3.1.2",
    "@types/koa-passport": "4.0.2",
    "@types/koa-router": "7.0.32",
    "@types/koa-session": "5.7.4",
    "@types/koa__cors": "2.2.2",
    "@types/node": "10.7.0",
    "@types/passport-facebook": "2.1.8",
    "@types/passport-local": "1.0.33",
    "nodemon": "1.18.5",
    "prettier": "1.14.3",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.6"
  },
  "scripts": {
    "postinstall": "bash ./setup-hooks.sh && npm run build",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'ts-node' ./src/server.ts",
    "start": "node ./dist/server.js",
    "start-local": "(npm run start-redis) & (npm run start-mongo) & npm start",
    "start-redis": "redis-server /usr/local/etc/redis.conf",
    "start-mongo": "mongod --config /usr/local/etc/mongod.conf",
    "build-ts": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && npm run build-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuriykuzin/koa-typescript-seed.git"
  },
  "keywords": [
    "koa",
    "typescript"
  ],
  "author": "Yuriy Kuzin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuriykuzin/koa-typescript-seed/issues"
  },
  "homepage": "https://github.com/yuriykuzin/koa-typescript-seed#readme"
}
